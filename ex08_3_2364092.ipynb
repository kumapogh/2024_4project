          "text": [
            "整数nを代入してください\n",
            "n:5\n",
            "5!=\n",
            "再帰:120\n",
            "ループ:120\n"
          ]
        }
      ],
      "source": [
        "def main():\n",
        " print(\"整数nを代入してください\")\n",
        " n = int(input(\"n:\"))\n",
        " print(str(n)+\"!=\")\n",
        " print(\"再帰:\"+str(saiki(n)))\n",
        " loop(n)\n",
        "\n",
        "def loop(n):\n",
        "  a = 1\n",
        "  for i in range(1,n+1):\n",
        "   a *= i\n",
        "  print(\"ループ:\"+str(a))\n",
        "\n",
        "def saiki(n):\n",
        "  if n == 0:\n",
        "    return 1\n",
        "  else:\n",
        "    return n*saiki(n-1)\n",
        "\n",
        "\n",
        "main()"
      ]
    }
  ]

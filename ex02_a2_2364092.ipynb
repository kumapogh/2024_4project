      "source": [
        "x = int(input('二進数に直したい自然数を入力してください'))\n",
        "list = []\n",
        "while x>0:\n",
        "   list.append( x % 2)\n",
        "   x = x // 2\n",
        "list.reverse()\n",
        "L = [str(a) for a in list]\n",
        "L = \"\".join(L)\n",
        "print(L)"
      ]

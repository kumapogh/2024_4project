        " def fact(n:int)->int:\n",
        "  fact=1\n",
        "  if n<0:\n",
        "    print(\"エラー:nの値が負です!!\")\n",
        "  else:\n",
        "    while n > 1:\n",
        "     fact = fact * n\n",
        "     n = n - 1\n",
        "    return fact\n",
        "\n",
        " fact(10)"
      ],
